xrenoise.use_xcursors:	1
xrenoise.display:	:0
xrenoise.display_resource:	0x00,0x02,0x47,0xE9,0x5A,0x8F,0x40,0xBE
xrenoise.executable_path:	/usr/bin/renoise

Xcursor.theme:	Neutral_Plus_Inverted
Xcursor.size:	10

Xft.dpi:	96
Xft.hintstyle:	hintslight
Xft.hinting:	true
Xft.rgba:	rgb
Xft.antialias:	true

URxvt.matcher.button:	1
URxvt.shading:	25
URxvt.cursorBlink:	false
URxvt.scrollBar:	false
URxvt.transparent:	false
URxvt.scrollBar_right:	false
URxvt.background:	#000000
URxvt.termName:	rxvt-unicode-256color
URxvt.urgentOnBell:	true
URxvt.saveLines:	2048 
URxvt.borderLess:	false
URxvt.dynamicColors:	on
URxvt.foreground:	#dcdccc
URxvt.visualBell:	true
URxvt.xftAntialias:	true
URxvt.buffered:	false
URxvt.loginShell:	true
URxvt.perl-ext-common:	default,matcher
URxvt.skipBuiltinGlyphs:	true
URxvt.cursorColor:	#eeeeee
URxvt.inheritPixmap:	true
URxvt.urlLauncher:	/usr/bin/firefox
URxvt.imLocale:	en_US.utf8

URxvt.font:	xft:Inconsolata:pixelsize=12
URxvt.bold:	xft:Inconsolata:pixelsize=12:bold
URxvt.italicFont:	xft:Inconsolata:pixelsize=12:italic
URxvt.bolditalicFont:	xft:Inconsolata:pixelsize=12:bold:italic

URxvt*foreground:	#dcdccc
URxvt*background:	#000000
*color2:	#6C7E55
*color7:	#CCCCCC
*color4:	#4c8ea1
*color5:	#956D9D
*color6:	#7C9AA6
*color3:	#CAAF2B
*color8:	#656565
*color13:	#C18FCB
*color15:	#FFFFFF
*color14:	#8FADBF
*color11:	#BFB556
*color12:	#6bc1d0
*color9:	#CC896D
*color10:	#7DB37D
*color0:	#222222

dzen2.font:       -*-Terminus-medium-r-normal-*-12-*-*-*-*-*-iso8859-2
dzen2.foreground: #bbbbbb
dzen2.background: #222222
